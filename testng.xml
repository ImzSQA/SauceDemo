<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Sauce Demo Test Suite">

    <test name="Login Test">
        <classes>
           <class name="org.imz.tests.LoginTest">
            <methods>
                <include name="testValidMultipleLogin"/>
                <include name="testLockedLogin"/>
            </methods>
            </class>
           <class name="org.imz.tests.LogoutTest">
              <methods>
                 <include name="ensureLoggedOut"/>
              </methods>
           </class>

        </classes>
    </test>
</suite>